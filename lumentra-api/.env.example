# Supabase
DATABASE_URL=postgresql://postgres:[YOUR-PASSWORD]@db.rrprimenrhubbxvcesxp.supabase.co:5432/postgres
SUPABASE_URL=https://[project].supabase.co
SUPABASE_ANON_KEY=

# Voice Provider - "vapi" (default) or "custom" (our SignalWire stack)
VOICE_PROVIDER=vapi

# Vapi (legacy/fallback)
VAPI_API_KEY=
VAPI_PHONE_ID=
VAPI_WEBHOOK_SECRET=

# ===== CUSTOM VOICE STACK =====
# Only required if VOICE_PROVIDER=custom

# SignalWire (Telephony) - https://signalwire.com
SIGNALWIRE_PROJECT_ID=
SIGNALWIRE_API_TOKEN=
SIGNALWIRE_SPACE_URL=yourspace.signalwire.com
SIGNALWIRE_PHONE_NUMBER=+1xxxxxxxxxx

# Deepgram (Speech-to-Text) - https://console.deepgram.com
DEEPGRAM_API_KEY=

# Groq (LLM) - https://console.groq.com
GROQ_API_KEY=

# Cartesia (Text-to-Speech) - https://play.cartesia.ai
CARTESIA_API_KEY=

# ===== END CUSTOM VOICE STACK =====

# Twilio (for SMS only)
TWILIO_ACCOUNT_SID=
TWILIO_AUTH_TOKEN=
TWILIO_PHONE_NUMBER=+1xxxxxxxxxx

# Server
PORT=3001
NODE_ENV=development
BACKEND_URL=https://your-ngrok-url.ngrok-free.dev

# Frontend URL (for CORS)
FRONTEND_URL=http://localhost:3000
